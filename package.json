{
	"name": "nodejs-stocks",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev": "next dev --port 3000",
		"build": "next build",
		"start": "next start --port 3000",
		"ngrokHome": "ngrok http --log=stdout --subdomain=dev-stocks 3000 > ngrok.log",
		"claude": "start cmd /k wsl",
		"claudeCommand": "claude --dangerously-skip-permissions",
		"npm-install": "npm install --legacy-peer-deps --save --force",
		"npm-remove-all": "del /f /q package-lock.json && rmdir /s /q node_modules",
		"lint": "next lint",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,mjs,json,css,md}\""
	},
	"keywords": [
		"stocks",
		"ai",
		"nextjs",
		"analysis"
	],
	"author": "Kevin",
	"license": "ISC",
	"dependencies": {
		"@anthropic-ai/sdk": "^0.32.1",
		"@google/generative-ai": "^0.24.1",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-dialog": "^1.1.2",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "1.1.1",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-toast": "^1.2.5",
		"@tanstack/react-query": "^5.62.3",
		"axios": "^1.7.9",
		"class-variance-authority": "^0.7.1",
		"classnames": "^2.5.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dotenv": "^16.4.7",
		"lightweight-charts": "^5.0.9",
		"lucide-react": "^0.483.0",
		"luxon": "^3.7.2",
		"mysql2": "^3.11.5",
		"next": "^14.1.4",
		"openai": "^5.10.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"recharts": "^2.15.4",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^3.4.16",
		"tailwindcss-animate": "^1.0.7",
		"tiny-invariant": "^1.3.1",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"eslint-config-next": "^14.2.4",
		"postcss": "^8.4.38",
		"prettier": "^3.3.2",
		"typescript": "^5.5.2"
	},
	"prettier": {
		"arrowParens": "always",
		"useTabs": true,
		"tabWidth": 3,
		"semi": true,
		"trailingComma": "none",
		"singleQuote": true,
		"bracketSpacing": false,
		"bracketSameLine": false,
		"jsxSingleQuote": false,
		"jsxBracketSameLine": true,
		"printWidth": 100
	}
}
